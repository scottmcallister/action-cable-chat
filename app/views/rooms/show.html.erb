<div class="chat-room container">
  <div class="navbar navbar-fixed-top navbar-light bg-faded">
    <a class="navbar-brand" href="#">Chat Room</a>
    <div class="pull-xs-right nav-item">
      <%= link_to "Sign Out", destroy_user_session_path, method: :delete %>
    </div>
  </div>

<div id="messages">
  <%= render @messages %>
</div>

</div>
<div class="text-field-wrapper">
  <form id="text-field" class="col-xs-10 col-xs-offset-1">
    <input type="text" data-behavior="room_speaker" placeholder="say something">
  </form>
</div>
